<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dreamstations.partyhistory" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".View.Act_Main"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".View.Act_Trip"
            android:label="红色旅游"/>
        <activity android:name=".View.Act_Trip_Detail"/>
        <activity android:name=".View.Act_Media"/>
        <activity android:name=".View.Media_Book_List"
            android:label="党史书籍"/>
        <activity android:name=".View.Media_Book_Detail"/>
    </application>

</manifest>
